_createElementVNode","style","onClick","$setup"],"sourceRoot":"","sources":["/Users/takanosayaka/Documents/GitHub/makeup_flowchart_vue3/src/components/ControlField.vue"],"sourcesContent":["<script setup>\nimport { useVueFlow } from '@braks/vue-flow'\nimport { useStore } from 'vuex'\n\nconst store = useStore()\n//const flowKey = 'example-flow'\n\nconst { nodes, addNodes, setNodes, setEdges, dimensions, setTransform } = useVueFlow()\n\nconst onSave = () => {\n  //localStorage.setItem(flowKey, JSON.stringify(toObject())) toObject\n  console.log(store.state.flowchart)\n}\n\nconst onRestore = () => {\n  const temp = store.state.flowchart\n  var temp2 = temp.replace(/\\n/g, '\\\\n')\n  const flow = JSON.parse(temp2)\n\n  if (flow) {\n    const [x = 0, y = 0] = flow.position\n    setNodes(flow.nodes)\n    setEdges(flow.edges)\n    setTransform({ x, y, zoom: flow.zoom || 0 })\n  }\n}\n\nconst onAdd = () => {\n  const id = nodes.value.length + 1\n  const newNode = {\n    id: `random_node-${id}`,\n    label: `Node ${id}`,\n    position: { x: Math.random() * dimensions.value.width, y: Math.random() * dimensions.value.height },\n  }\n  addNodes([newNode])\n}\n</script>\n\n<template>\n  <div class=\"save__controls\">\n    <button style=\"background-color: #33a6b8\" @click=\"onSave\">save</button>\n    <button style=\"background-color: #113285\" @click=\"onRestore\">restore</button>\n    <button style=\"background-color: #6f3381\" @click=\"onAdd\">add node</button>\n  </div>\n</template>\n"]}]}