setData","effectAllowed","setImage","get","require","state","partNum"],"sources":["/Users/takanosayaka/Documents/GitHub/makeup_flowchart_vue3/src/components/NodeField.vue"],"sourcesContent":["<script setup>\nimport { computed } from \"@vue/runtime-core\"\nimport { useStore } from 'vuex'\n\n//変数設定\nconst images = [\"base\", \"eyebrow\", \"eye\", \"nose\", \"mouth\"]\nconst store = useStore()\n\nconst onDragStart = (event, nodeType, datas) => {\n  if (event.dataTransfer) {\n    event.dataTransfer.setData('application/vueflow', nodeType, datas)\n    event.dataTransfer.effectAllowed = 'move'\n  }\n}\n\nconst setImage = computed({\n    get: () => (\n        require('../assets/icon/'+images[store.state.partNum]+'.png')\n    ),\n})\n</script>\n\n<template>\n  <aside>\n    <div class=\"nodes\">\n      <div class=\"vue-flow__node-input\" :draggable=\"true\" @dragstart=\"onDragStart($event, 'input')\">メイク開始</div>\n      <div class=\"vue-flow__node-output\" :draggable=\"true\" @dragstart=\"onDragStart($event, 'output')\">メイク終了</div>\n      <div class=\"node-makeup\" style=\"display: flex\" :draggable=\"true\"\n        @dragstart=\"onDragStart($event, 'customMake')\"\n      >\n        <img id=\"micon\" :src=\"setImage\" />\n        <template v-if=\"this.$store.state.typeName != '指定なし'\">\n          <p id=\"mtitle\">{{this.$store.state.itemName+\"(\"+this.$store.state.typeName+\")\"}}</p>\n        </template>\n        <template v-else>\n          <p id=\"mtitle\">{{this.$store.state.itemName}}</p>\n        </template>\n      </div>\n      <template v-if=\"this.$store.state.turn == 2\">\n        <div class=\"branch-node\" :draggable=\"true\" @dragstart=\"onDragStart($event, 'customBranch')\">\n          <div class=\"diamond\">\n            <div class=\"diamond-inner\">\n              <p id=\"btitle\">気合い</p>\n            </div>\n          </div>\n        </div>\n      </template>\n    </div>\n  </aside>\n</template>\n\n<style>\naside{\n  height: 280px;\n  padding: 15px 10px;\n  color:#fff;\n  font-size: 12px;\n  font-weight: 700;\n  background: rgba(16,185,129,.75);\n  border-right: 1px solid #eee;\n}\naside .nodes>*{\n  margin-bottom: 10px;\n  cursor: grab;\n  font-weight: 500;\n  -webkit-box-shadow: 5px 5px 10px 2px rgba(0,0,0,.25);\n  box-shadow: 5px 5px 10px 2px #00000040\n}\n\n/* MakeNode */\n.node-makeup{\n  padding: 10px;\n  min-width: 160px;\n  max-width: 170px;\n  text-align: center;\n  font-size: 12px;\n  color: var(--vf-node-text);\n  background: var(--vf-node-bg);\n  border-radius: 3px;\n  border-width: 1px;\n  border-style: solid;\n  border-color: var(--vf-node-color);\n}\n#micon{\n  width: 25px;\n  height: 25px;\n  padding: 0px 0px 0px 15px; /* 上 | 右 | 下 | 左 */\n}\n#mtitle{\n  padding-top: 3px;\n  margin: 0px 0px 0px 20px; /* 上 | 右 | 下 | 左 */\n  white-space: pre-wrap;\n}\n\n/* BranchNode */\n.branch-node{\n  width: 160px;\n  padding: 0px;\n  margin: 0px;\n}\n.diamond {\n  width: 80px;\n  height: 80px;\n  position: relative;\n  transform: rotate(135deg) skew(20deg, 20deg);\n  margin: 0px;\n  padding: 0px;\n  left: 39px;\n  border-width: 1px;\n  border-radius: 3px;\n  border-style: solid;\n  background: var(--vf-node-bg);\n  border-color: var(--vf-node-color);\n}\n.diamond-inner {\n  position: absolute;\n  width: 160%;\n  transform: skew(-20deg, -20deg) rotate(-135deg); /* 傾き・回転をリセット */\n  display: flex;\n}\n#btitle{\n  padding-top: 20px;\n  padding-left: 5px;\n  margin: 5px 0px 0px 50px; /* 上 | 右 | 下 | 左 */\n  font-size: 14px;\n  color: var(--vf-node-text);\n  white-space: pre-wrap;\n}\n</style>"]}]}