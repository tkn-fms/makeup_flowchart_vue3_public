s/GitHub/makeup_flowchart_vue3/src/components/SeparateNode.vue","sourceRoot":"","sourcesContent":["<script setup>\nimport { Handle, Position } from '@braks/vue-flow'\nimport { defineProps, computed } from 'vue'\n\nconst images = [\"base\", \"eyebrow\", \"eye\", \"nose\", \"mouth\"]\nconst props = defineProps({\n    label: {\n        type: String,\n        required: true,\n    },\n    data: {\n        required: true,\n    },\n})\n\nconst setUrl = computed({\n    get: () => (\n        require('../assets/icon/'+images[props.data.partNum]+'.png')\n    ),\n})\n</script>\n\n<template>\n  <div class=\"separate-node\">\n    <Handle type=\"target\" :position=\"Position.Top\" />\n    <div class=\"diamond\">\n      <div class=\"diamond-inner\">\n        <img id=\"icon\" :src=\"setUrl\">\n        <p id=\"title\">{{props.label}}</p>\n      </div>\n    </div>\n    <Handle type=\"source\" :position=\"Position.Bottom\" />\n  </div>\n</template>\n\n<style>\n.separate-node{\n  margin: 0px;\n  padding: 0px;\n}\n.diamond {\n  width: 100px;\n  height: 100px;\n  position: relative;\n  transform: rotate(135deg) skew(20deg, 20deg);\n  margin: 0px;\n  padding: 0px;\n  border-width: 1px;\n  border-radius: 3px;\n  border-style: solid;\n  background: var(--vf-node-bg);\n  border-color: var(--vf-node-color);\n}\n.diamond-inner {\n  position: absolute;\n  width: 160%;\n  top: 40px;\n  left: -35px;\n  transform: skew(-20deg, -20deg) rotate(-135deg); /* 傾き・回転をリセット */\n  font-size: 12px;\n  display: flex;\n}\n#icon{\n  width: 25px;\n  height: 25px;\n  padding: 5px 0px 0px 0px; /* 上 | 右 | 下 | 左 */\n}\n#title{\n  margin: 0px 0px 0px 5px;\n  padding-top: 7px;\n}\n</style>"]}]}