makeup\" style=\"display: flex\" :draggable=\"true\"\n        @dragstart=\"onDragStart($event, 'customMake')\"\n      >\n        <img id=\"icon\" :src=\"setImage\" />\n        <template v-if=\"store.state.typeName != '指定なし'\">\n          <p id=\"title\">{{this.$store.state.itemName+\"(\"+this.$store.state.typeName+\")\"}}</p>\n        </template>\n        <template v-else>\n          <p id=\"title\">{{this.$store.state.itemName}}</p>\n        </template>\n      </div>\n      <div class=\"separate-node\" :draggable=\"true\" @dragstart=\"onDragStart($event, 'customBranch')\">\n        <div class=\"diamond\">\n          <div class=\"diamond-inner\">\n            <img id=\"sicon\" :src=\"setImage\">\n            <p id=\"stitle\">{{this.$store.state.itemName+\"(\"+this.$store.state.typeName+\")\"}}</p>\n          </div>\n        </div>\n      </div>\n    </div>\n  </aside>\n</template>\n\n<style>\naside{\n  color:#fff;\n  font-weight: 700;\n  border-right: 1px solid #eee;\n  padding: 15px 10px;\n  font-size: 12px;\n  background: rgba(16,185,129,.75);\n}\naside .nodes>*{\n  margin-bottom: 10px;\n  cursor: grab;\n  font-weight: 500;\n  -webkit-box-shadow: 5px 5px 10px 2px rgba(0,0,0,.25);\n  box-shadow: 5px 5px 10px 2px #00000040\n}\n\n/* MakeNode */\n.node-makeup{\n  padding: 10px;\n  border-radius: 3px;\n  min-width: 160px;\n  max-width: 170px;\n  font-size: 12px;\n  color: var(--vf-node-text);\n  text-align: center;\n  border-width: 1px;\n  border-style: solid;\n  background: var(--vf-node-bg);\n  border-color: var(--vf-node-color);\n}\n#icon{\n  width: 25px;\n  height: 25px;\n  padding: 0px; /* 上 | 右 | 下 | 左 */\n}\n#title{\n  white-space: pre-wrap;\n  margin: 0px 0px 0px 5px;\n  padding-top: 3px;\n}\n/* node-separate */\n.separate-node{\n  width: 200px;\n  margin: 0px;\n  padding: 0px;\n}\n.diamond {\n  width: 100px;\n  height: 100px;\n  position: relative;\n  transform: rotate(135deg) skew(20deg, 20deg);\n  margin: 0px;\n  padding: 0px;\n  left: 50px;\n  border-width: 1px;\n  border-radius: 3px;\n  border-style: solid;\n  background: var(--vf-node-bg);\n  border-color: var(--vf-node-color);\n}\n.diamond-inner {\n  position: absolute;\n  width: 160%;\n  top: 40px;\n  left: -35px;\n  transform: skew(-20deg, -20deg) rotate(-135deg); /* 傾き・回転をリセット */\n  font-size: 12px;\n  display: flex;\n}\n#sicon{\n  width: 25px;\n  height: 25px;\n  padding: 5px 0px 0px 0px; /* 上 | 右 | 下 | 左 */\n}\n#stitle{\n  color: var(--vf-node-text);\n  white-space: pre-wrap;\n  margin: 0px 0px 0px 5px;\n  padding-top: 7px;\n}\n</style>"],"names":[],"mappings":"AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAC5C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAC/B;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;;;;;;AAJQ;AAKd,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAC1D,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AACxB;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AACjD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAC3B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,